# Unit tests for generalIO ~ achel,proc,faucet,unitTest,hidden

defineTest createProcFaucet - simple execution,
	setTestType legacy
	createProcFaucet procDemo,echo hi
	createDumpFaucet procDump,ProcDemo
	createPipe procDemo,procDump
	
	deliverAll
	
	expect hi,~!ProcDemo,0!~
	
	deleteFaucet procDemo
	deleteFaucet procDump

