# Unit tests for the Codes package. ~ Codes,unitTest,codes,hidden

defineTest colorText - simple,
	colorText Local,colorCode,hello
	expectNot ,~!Local,colorCode!~

defineTest colorText - simple repeat,
	colorText Local,colorCode,hello
	expectNot ,~!Local,colorCode!~

